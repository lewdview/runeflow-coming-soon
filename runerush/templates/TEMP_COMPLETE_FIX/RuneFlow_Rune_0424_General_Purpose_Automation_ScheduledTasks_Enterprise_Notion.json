      "parameters": {
        "jsCode": "// ç¢ºä¿ items æ˜¯å¦æœ‰å…§å®¹\nif (!items || items.length === 0) {\n console.log('No items found');\n return [];\n}\n\n// å–å¾—æ‰€æœ‰è²¼æ–‡\nconst posts = items.map(item => item.json).flat();\nconsole.log('Number of posts:', posts.length);\n\n// å–å¾—ç¬¬ä¸€ç¯‡è²¼æ–‡çš„åŸºæœ¬è³‡è¨Š\nconst firstPost = posts[0] || {};\n\n// ç”Ÿæˆ blocks çµæ§‹\nconst blocks = [];\n\n// è™•ç†æ¯å€‹è²¼æ–‡\nposts.forEach(post => {\n // å¦‚æœæœ‰æ–‡å­—ï¼ŒåŠ å…¥æ–‡å­—å€å¡Š\n if (post.text) {\n blocks.push({\n object: \"block\",\n type: \"paragraph\",\n paragraph: {\n rich_text: [{\n type: \"text\",\n text: { content: post.text }\n }]\n }\n });\n }\n \n // å¦‚æœæœ‰åª’é«”é€£çµï¼ŒåŠ å…¥ embed å€å¡Š\n if (post.media_urls && post.media_urls.length > 0) {\n post.media_urls.forEach(url => {\n blocks.push({\n object: \"block\",\n type: \"embed\",\n embed: { url }\n });\n });\n }\n});\n\n// åˆä½µåŸºæœ¬è³‡è¨Šå’Œ blocks\nconst combinedPost = {\n id: firstPost.id || '',\n permalink: firstPost.permalink || '',\n username: firstPost.username || '',\n timestamp: firstPost.timestamp || '',\n blocks\n};\n\n// å°‡çµæœè½‰æ›ç‚º n8n æ‰€éœ€æ ¼å¼\nreturn [{ json: combinedPost }];"
      },
